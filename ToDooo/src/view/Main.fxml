<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="vBoxMain" onKeyPressed="#onShortcutKey" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <children>
      <AnchorPane fx:id="anPaneHeader" prefHeight="100.0" prefWidth="750.0">
         <children>
            <fx:include fx:id="header" source="Header.fxml" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="anPaneBody" prefHeight="500.0" prefWidth="750.0">
         <children>
            <fx:include fx:id="body" source="Body.fxml" />
         </children>
      </AnchorPane>
   </children>
</VBox>
